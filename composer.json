{
    "name": "erunion/mill",
    "description": "â˜´ An annotation-based DSL for documenting a REST API.",
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Hedges",
            "email": "aaron@dashron.com"
        },
        {
            "name": "Jon Ursenbach",
            "email": "jon@ursenba.ch"
        }
    ],
    "minimum-stability": "RC",
    "bin": ["bin/mill"],
    "require": {
        "php": ">=7.2.0",
        "ext-intl": "*",
        "ext-xml": "*",
        "composer/semver": "^1.4",
        "cocur/slugify": "^4.0",
        "dflydev/dot-access-data": "^1.0",
        "gossi/docblock": "^1.5",
        "league/flysystem": "^1.0",
        "nicmart/string-template": "^0.1.1",
        "ocramius/package-versions": "^1.1",
        "pimple/pimple": "^3.0",
        "symfony/console": "^3.2 || ^4.0"
    },
    "require-dev": {
        "league/flysystem-memory": "^1.0",
        "phpunit/phpunit": "^7.0",
        "psalm/plugin-phpunit": "^0.5",
        "squizlabs/php_codesniffer": "^3.0",
        "vimeo/psalm": "^3.0"
    },
    "suggest": {
        "ext-xml": "Required for config file processing."
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "Mill\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mill\\Examples\\": "resources/examples",
            "Mill\\Tests\\": "tests/",
            "Mill\\Tests\\Fixtures\\": "tests/_fixtures"
        }
    }
}
